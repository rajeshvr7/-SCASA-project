@model SCASA.Models.ModelClasses.UserMasterDisplay;
@{
}
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="header-icon">
            <i class="fa fa-dashboard"></i>
        </div>
        <div class="header-title">
			<div class="row">
				<div class="col-md-9">
					<h1>Profile</h1>
					<small>Dashboard / Profile</small>
				</div>
				<div class="col-md-3">
					<div class="btn-group float-right" role="group">
						<div class="buttonexport" id="buttonlist">
							<a class="btn btn-add" href="~/Staff/StaffList">
								<i class="fa fa-reply mr-2"></i>Back
							</a>
						</div>
					</div>
				</div>
			</div>
        </div>
    </section>
    <!-- Main content -->
    <div class="content">
        <div class="row">
            <!-- Form controls -->
            <div class="col-sm-12">
                <div class="card mb-0">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="profile-view">
                                    <div class="profile-img-wrap">
                                        <div class="profile-img">
                                            @if (Model.ProfileImage != null)
                                            {
                                                <img alt="" src="~/ProductImages/@Model.ProfileImage">
                                            }
                                            else
                                            {
                                                <img alt="" src="~/ProductImages/dummy.png">
                                            }

                                        </div>
                                    </div>
                                    <div class="profile-basic">
                                        <div class="row">
                                            <div class="col-md-5">
                                                <div class="profile-info-left">
                                                    <h4 class="user-name m-t-0 mb-4 font-weight-bold">@Model.UserName</h4>
                                                    <h6 class="text-muted">@Model.UserTypeName</h6>
                                                    <h5 class="text-danger" style="padding-top:5px;">Status :  @Model.CurrentStatus</h5>
                                                    @*<small class="text-muted">Manager</small>*@
                                                    <div class="staff-id">Employee Code : @Model.EmployeeCode </div>
                                                    @{
                                                        DateTime dj = Convert.ToDateTime(Model.DateOfJoin);
                                                    }
                                                    <div class="small doj text-muted">Date of Join : @dj.ToString("D")</div>
                                                    <div class="small doj text-muted">Status : @Model.CurrentStatus</div>
                                                </div>
                                            </div>
                                            <div class="col-md-7">
                                                <ul class="personal-info">
                                                    <li>
                                                        <div class="title">Phone:</div>
                                                        <div class="text">@Model.MobileNumber</div>
                                                    </li>
                                                    <li>
                                                        <div class="title">Email:</div>
                                                        <div class="text">@Model.EmailId</div>
                                                    </li>
                                                    @{
                                                        DateTime db = Convert.ToDateTime(Model.DateOfBirth);
                                                    }
                                                    <li>
                                                        <div class="title">Birthday:</div>
                                                        <div class="text">@db.ToString("d")</div>
                                                    </li>
                                                    <li>
                                                        <div class="title">Address 1 :</div>
                                                        <div class="text">@Model.Address1</div>
                                                        @*<div class="text">@Model.Address2</div>*@
                                                    </li>
                                                    @if (Model.Address2 != null)
                                                     {
                                                        <li>
                                                            <div class="title">Address 2 :</div>
                                                            <div class="text">@Model.Address2</div>
                                                        </li>
                                                     }
                                                     <li>
                                                        <div class="title">Gender:</div>
                                                        <div class="text">@Model.Gender</div>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="pro-edit"><a class="edit-icon" href="~/Staff/AddStaff?id=@Model.UserId"><i class="fa fa-pencil"></i></a></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-4">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                Other Details
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-6">
                                        <ul class="personal-info">
                                            <li>
                                                <div class="title">Country :</div>
                                                <div class="text">@Model.CountryId_Name</div>
                                            </li>
                                            <li>
                                                <div class="title">State :</div>
                                                <div class="text">@Model.StateId_Name</div>
                                            </li>
                                            <li>
                                                <div class="title">City :</div>
                                                <div class="text">@Model.CityId_Name</div>
                                            </li>
                                            <li>
                                                <div class="title">Zip Code :</div>
