                                            </li>
                                            <li>
                                                <div class="title">Reffered By Number :</div>
                                                <div class="text">@Model.ReferredByMobile</div>
                                            </li>
                                            <li>
                                                <div class="title">Emergency Contact Number :</div>
                                                <div class="text">@Model.EmergencyContactNumber</div>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="col-md-6">
                                        <ul class="personal-info">
                                            <li>
                                                <div class="title">Max Discount Percentage :</div>
                                                <div class="text">@Model.MaxDiscoutPercentage</div>
                                            </li>
                                            <li>
                                                <div class="title">Pan Number :</div>
                                                <div class="text">@Model.PANNumber</div>
                                                <span> <a href="~/ProductImages/@Model.PanCardUploaded" target="_blank">Front</a></span>
                                            </li>
                                            <li>
                                                <div class="title">Pf Number :</div>
                                                <div class="text">@Model.PFNumber</div>
                                            </li>
                                            <li>
                                                <div class="title">UAN Number :</div>
                                                <div class="text">@Model.UANNumber</div>
                                            </li>
                                            <li>
                                                <div class="title">ESI Number :</div>
                                                <div class="text">@Model.ESICNumber</div>
                                            </li>
                                            <li>
                                                <div class="title">BioMetric Id :</div>
                                                <div class="text">@Model.BioMetricId</div>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-4">
