       =====================================================================-->
    <!-- Start page Label Plugins
       =====================================================================-->
    <!-- Emojionearea -->
    <link href="~/WebApp/assets/plugins/emojionearea/emojionearea.min.css" rel="stylesheet" />
    <!-- Monthly css -->
    <link href="~/WebApp/assets/plugins/monthly/monthly.css" rel="stylesheet" />
    <!-- End page Label Plugins
       =====================================================================-->
    <!-- Start Theme Layout Style
       =====================================================================-->
    <!-- Theme style -->
    <link href="~/WebApp/assets/dist/css/stylecrm.css" rel="stylesheet" />
