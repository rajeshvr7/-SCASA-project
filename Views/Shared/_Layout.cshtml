    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.3/css/dataTables.bootstrap4.min.css" />
    <script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.3/js/dataTables.bootstrap4.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#mydatatable').DataTable();
        });
    </script>

</head>
<body class="hold-transition sidebar-mini sidebar-collapse">
	<!--preloader-->
	<div id="preloader">
		<div id="status"></div>
	</div>
	<!-- Site wrapper -->
	<div class="wrapper">
		<header class="main-header">
			<a href="~/EmpireHome/Dashboard" class="logo">
				<!-- Logo -->
				<span class="logo-mini">
					<img src="~/WebApp/assets/dist/img/mini-logo.png" alt="">
				</span>
				<span class="logo-lg">
					<img src="~/WebApp/assets/dist/img/logo.png" alt="">
				</span>
			</a>
			<!-- Header Navbar -->
			<nav class="navbar navbar-expand py-0">
				<a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
					<!-- Sidebar toggle button-->
					<span class="sr-only">Toggle navigation</span>
					<span class="pe-7s-keypad"></span>
				</a>
				<!-- searchbar-->
				<!--<a href="#search"><span class="pe-7s-search"></span></a>-->
				<div id="search">
					<button type="button" class="close">×</button>
					<form>
						<input type="search" value="" placeholder="Search.." />
						<button type="submit" class="btn btn-add">Search...</button>
					</form>
				</div>
				<div class="collapse navbar-collapse navbar-custom-menu">
					<ul class="navbar-nav ml-auto">
						<!-- Orders -->
						<!-- Messages -->
					 
						<!-- Notifications -->
                       
                            

                            <span id="NotificationsList"></span>
                       
                        
						<!-- Tasks -->
						<!-- Help -->
						 
						<!-- User -->
						<li class="nav-item dropdown dropdown-user">
							<a class="nav-link" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
								<img src="~/WebApp/assets/dist/img/avatar5.png" class="rounded-circle" width="50" height="50" alt="user">
							</a>

							<div class="dropdown-menu drop_down">
								<div class="menus">
                                    <a class="dropdown-item" href="~/Home/CheckForProfile">
                                        <i class="fa fa-user"></i>
                                        @if (ViewBag.LoggedUser != null)
                                        {
                                            @ViewBag.LoggedUser.userName
                                        }
                                        else
                                        {
                                            <b>EmpireHome</b>
                                        }
                                    </a>

									<a class="dropdown-item" href="~/Authenticate/Logout"><i class="fa fa-sign-out"></i> Signout</a>
								</div>
							</div>
						</li>
					</ul>
				</div>
			</nav>
		</header>
		<input id="loggedUserId" type="hidden" value="@ViewBag.LoggedUser.userId" />
		<!-- =============================================== -->
		<!-- Left side column. contains the sidebar -->
		<aside class="main-sidebar">
			<!-- sidebar -->
			<div class="sidebar">
				<!-- sidebar menu -->
               <span id="sideBarMenu">
</span>
				</div>
			<!-- /.sidebar -->
		</aside>
		<!-- =============================================== -->
		<!-- Content Wrapper. Contains page content -->
		@RenderBody()
		<!-- /.content-wrapper -->
		<footer class="main-footer">
			Inventory & Sales Management System (ISMS) | <strong>Copyright &copy; @DateTime.Now.Year <a href="#">Empire Home</a>.</strong> All rights reserved. |
			Version. 1.0.0

		</footer>
	</div>
	<!-- /.wrapper -->
	<!-- Start Core Plugins
	   =====================================================================-->
	<!-- Bootstrap proper -->
	<script src="~/WebApp/assets/bootstrap/js/popper.min.js"></script>
	<!-- lobicard ui min js -->
	<script src="~/WebApp/assets/plugins/lobipanel/js/jquery-ui.min.js"></script>
	<!-- lobicard ui touch-punch-improved js -->
	<script src="~/WebApp/assets/plugins/lobipanel/js/jquery.ui.touch-punch-improved.js"></script>
	<!-- lobicard tether min js -->
	<script src="~/WebApp/assets/plugins/lobipanel/js/tether.min.js"></script>
	<!-- Bootstrap -->
	<script src="~/WebApp/assets/bootstrap/js/bootstrap.min.js"></script>
	<!-- lobicard js -->
	<script src="~/WebApp/assets/plugins/lobipanel/js/lobicard.js"></script>
	<!-- lobicard highlight js -->
	<script src="~/WebApp/assets/plugins/lobipanel/js/highlight.js"></script>
	<!-- Pace js -->
	<script src="~/WebApp/assets/plugins/pace/pace.min.js"></script>
	<!-- NIceScroll -->
	<script src="~/WebApp/assets/plugins/slimScroll/jquery.nicescroll.min.js"></script>
	<!-- FastClick -->
	<script src="~/WebApp/assets/plugins/fastclick/fastclick.min.js"></script>
	<!-- CRMadmin frame -->
	<script src="~/WebApp/assets/dist/js/custom.js"></script>
